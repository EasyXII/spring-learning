<mapper namespace="cn.mapper.ForumMapper.xml">
    <insert id="insertForum" parameterType="cn.domain.Forum" resultType="Integer">
           insert into t_forum (forum_id,forum_name,description,image) value(forum_id,#{forum_name},#{description},#{image})
    </insert>
</mapper>